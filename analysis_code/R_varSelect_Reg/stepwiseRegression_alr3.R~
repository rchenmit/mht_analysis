library(alr3)
set.seed(1)
test <- sample(1:202,150,replace=FALSE)
ais0 <- with(ais, dta.frame(LBM = LBM, logSSF = log(SSF), logWt = log(Wt), Sex = Sex, logHg=log(Hg), logHt = log(Ht), logRCC = log(RCC), logHc = log(Hc), logWCC = loc(WCC), logFerr=log(Ferr)))
ais0 <- as.data.frame(scale(ais0))
m0 <- lm(LBM~1,ais0, subset=test)
m1 <- update(m0,LBM~logWt+logSSF+logRCC+Sex+logHg+logHt+logWCC+logHc+logFerr) > (f <- as.formula(paste("~",paste(names(coef(m1))[-1],collapse="+"))))
